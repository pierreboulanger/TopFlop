<div class="container">
  <div class="text-center">
    <h1>Create a new Team !</h1>

    <%= simple_form_for @team do |f| %>
      <%= f.input :name %>
      <%#= f.input :photos, as: :attachinary %>
      <%= f.button :submit, class:"btn btn-success btn-full-width" %>
    <% end %>
  </div>
</div>

<div class="back fixed-bottom">
  <%= link_to "Back", request.referer, class:"btn btn-danger btn-full-width" %>
</div>
